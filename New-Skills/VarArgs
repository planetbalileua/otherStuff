//http://docs.oracle.com/javase/1.5.0/docs/guide/language/varargs.html

//example:
  public static void main(String[] args)
  {
		System.out.println(res(20,20,100));
    
  }
  public static double res(int... r){
        double subtotal = 0;
        for(double sub : r){
            subtotal = subtotal + (1/sub) ;
        }
        return (1/subtotal);
    }
  }
